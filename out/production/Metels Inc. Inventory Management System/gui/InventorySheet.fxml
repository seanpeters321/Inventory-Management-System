                stylesheets="@../resources/stylesheets/StyleSheet.css" text="Back"/>
        <Button layoutX="105.0" layoutY="64.0" mnemonicParsing="false" onAction="#refresh"
                stylesheets="@../resources/stylesheets/StyleSheet.css" text="Refresh"/>
    </children>
</AnchorPane>
